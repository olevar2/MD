# Example environment variables for feature-store-service
# Copy this file to .env and fill in the actual values.

# Logging level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Service port
PORT=8001

# Database connection string (replace with your actual database credentials)
DB_CONNECTION_STRING=postgresql://user:password@localhost:5432/features

# Redis connection string for caching
REDIS_URL=redis://localhost:6379

# API key for securing service-to-service communication (replace with a strong key)
API_KEY=replace_with_secure_api_key

# Days to keep feature data
DATA_RETENTION_DAYS=90

SERVICE_NAME=data-pipeline-service
APP_VERSION=0.1.0
DEBUG_MODE=False
LOG_LEVEL=DEBUG
DB_USER=postgres
DB_PASSWORD=postgres
DB_HOST=localhost
DB_PORT=5432
DB_NAME=data_pipeline_db
DB_POOL_SIZE=20
DB_SSL_REQUIRED=False
KAFKA_BOOTSTRAP_SERVERS=localhost:9092
KAFKA_CONSUMER_GROUP_PREFIX=data-pipeline
KAFKA_AUTO_CREATE_TOPICS=True
KAFKA_PRODUCER_ACKS=all
OANDA_API_KEY=your_oanda_api_key_here
OANDA_ACCOUNT_ID=your_oanda_account_id_here
MAX_REQUESTS_PER_MINUTE=60
MAX_RETRIES=3
RETRY_DELAY_SECONDS=5
TIMEOUT_SECONDS=30
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=
REDIS_TIMEOUT=5
USE_OBJECT_STORAGE=False
OBJECT_STORAGE_ENDPOINT=https://your-s3-endpoint.com
OBJECT_STORAGE_KEY=your_object_storage_key_here
OBJECT_STORAGE_SECRET=your_object_storage_secret_here
OBJECT_STORAGE_BUCKET=your_bucket_name_here
HOST=localhost
PORT=8001
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s
FEATURE_ENABLE_GPU_ACCELERATION=false
FEATURE_ENABLE_DISTRIBUTED_COMPUTING=false
FEATURE_ENABLE_ADVANCED_INDICATORS=true

# Example environment variables for e2e testing framework
# Copy this file to .env and fill in the actual values.

# Logging level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Test environment (local, development, staging)
TEST_ENV=local

# JSON string containing service URLs
# Example: {"trading-gateway": "http://localhost:8004", "analysis-engine": "http://localhost:8002"}
SERVICE_URLS={"trading-gateway": "http://localhost:8004", "analysis-engine": "http://localhost:8002", "feature-store": "http://localhost:8001"}

# Credentials for test accounts (replace with secure test credentials)
# Format: JSON string with usernames and passwords
TEST_ACCOUNT_CREDENTIALS={"test_trader": "replace_with_secure_password", "test_analyst": "replace_with_secure_password"}

# Path for test reports
REPORT_PATH=./reports

# Number of parallel test executions
PARALLEL_EXECUTION=3

# Take screenshots on test failure (true/false)
SCREENSHOTS_ON_FAILURE=true

# Default timeout for test operations (in seconds)
TIMEOUT_SECONDS=30
